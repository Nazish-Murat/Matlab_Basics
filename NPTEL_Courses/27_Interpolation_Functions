clear all; close all; clc;
%%  Interpolation
% Speed in 1.5 minute
t = 0:10:90;
s = [45 32 0 0 7 12 20 15 29 55];
plot(t,s,'bo');
hold on

% Interpolation using Spline
tI = 0:90;
sI = spline(t,s,tI);
plot(tI,sI,'-k');
hold on

% Interpolation using PCHIP
sI_pc = pchip(t,s,tI);
plot(t,s,'--r');
hold on


